{
  "name": "developper-testing-plaform",
  "version": "1.0.0",
  "main": "dist/server.js",
  "author": "Rodolphe Renaux <rrenaux@idobs.net>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev:webpack": "NODE_ENV=development webpack --mode=development -w",
    "dev:server": "nodemon ./dist/server.js",
    "dev": "npm-run-all --parallel dev:*",
    "build": "NODE_ENV=production webpack --mode=production",
    "test": "jest --coverage"
  },
  "jest": {
    "globals": {
      "__BASEPATH__": "<rootDir>"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/__tests__/setup.ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/__tests__/setup.ts"
    ],
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{ts,tsx}",
      "<rootDir>/server/**/*.{ts,tsx}",
      "<rootDir>/web/**/*.{ts,tsx}"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/server/index.ts",
      "<rootDir>/web/index.ts"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(scss|sass|css|less)$": "identity-obj-proxy"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.176",
    "@types/morgan": "^1.9.3",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/react-helmet": "^6.1.4",
    "@types/react-router-dom": "^5.3.2",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.5.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.3.1",
    "mini-css-extract-plugin": "^2.4.4",
    "multi-loader": "^0.1.0",
    "nodemon": "^2.0.14",
    "npm-run-all": "^4.1.5",
    "null-loader": "^4.0.1",
    "regenerator-runtime": "^0.13.9",
    "sass": "^1.43.4",
    "sass-loader": "^12.3.0",
    "style-loader": "^3.3.1",
    "superagent": "^6.1.0",
    "supertest": "^6.1.6",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.4",
    "webpack": "^5.60.0",
    "webpack-cli": "^4.9.1",
    "webpack-manifest-plugin": "^4.0.2",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^5.3.0",
    "rotating-file-stream": "^2.1.6"
  }
}
